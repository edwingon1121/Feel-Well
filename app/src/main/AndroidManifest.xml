<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="itp341.gonzalez.edwin.feelwellapp">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".transition">

        <activity android:name=".splash" android:theme="@style/splashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".Navigation"></activity>
        <activity android:name=".ListActivity"></activity>
        
        <activity android:name=".Thoughts.ThoughtsAdd"
            android:parentActivityName=".ListActivity"></activity>

        <activity android:name=".Food.FoodAdd"
            android:parentActivityName=".ListActivity"></activity>

        <activity android:name=".Fitness.FitnessAdd"
            android:parentActivityName=".ListActivity"></activity>

    </application>

</manifest>